<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

	<changeSet id="1" author="jonas">
		<createTable tableName="Device">
			<column name="ID" type="INTEGER" autoIncrement="true">
				<constraints primaryKey="true" nullable="false"/>
			</column>
			<column name="internalId" type="VARCHAR(50)" defaultValue="" />
			<column name="name" type="VARCHAR(255)" defaultValue="" />
			<column name="type" type="VARCHAR(100)" defaultValue="" />
			<column name="added" type="TIMESTAMP" defaultValueDate="NULL" />
		</createTable>
	</changeSet>
	
</databaseChangeLog>